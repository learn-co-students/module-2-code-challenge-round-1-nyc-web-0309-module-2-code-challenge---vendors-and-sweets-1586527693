<% if flash[:errors] %>
<% flash[:errors].each do |errors| %>
<%= errors %><br>
<% end %>
<% end %>

<h3> VendorSweet Create Form </h3>

<%= form_for @vendorsweet do |f| %>
<%= f.label :vendor%>
<%= f.collection_select(:vendor_id, @vendors, :id, :name) %>

<%= f.label :sweet%>
<%= f.collection_select(:sweet_id, @sweets, :id, :name) %>

<%= f.label :price%>
<%= f.number_field :price %>

<%= f.submit%>
<% end %>
