
<h4>Vender Sweet Create Page</h4>

<% if flash[:errors]%>
    <% flash[:errors].each do |error|%>
    <%= error%>
    <%end %>
<% end %>


<%= form_for @vendor_sweet do |f|%>
<%= f.label :price%><br>
<%= f.number_field :price%><br>
<br>

<%=f.collection_select :vendor_id, Vendor.all.uniq, :id, :name %><br>
<br>
<%=f.collection_select :sweet_id, Sweet.all.uniq, :id, :name %><br>
<br>

<%= f.submit %>

<% end %>
