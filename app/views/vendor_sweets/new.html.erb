<h1>Create a New VendorSweet</h1>

<% if flash[:messages]%>
    <% flash[:messages].each do |message|%>
    <%= message %> <br>
    <% end %>
<% end %>

<%= form_for @vendorsweet do |f| %>
    <%= f.label :vendor_id %>
    <%= f.collection_select :vendor_id, Vendor.all, :id, :name %> <br>

    <%= f.label :sweet_id %>
    <%= f.collection_select :sweet_id, Sweet.all, :id, :name %> <br>

    <%= f.label :price %>
    <%= f.number_field :price %> <br>

    <%= f.submit %>
<% end %>